data = [i for i in range(1,21)]
print(data)

odd = [j for j in data if j%2 != 0]
print(odd)

even = [k for k in data if k%2 == 0]
print(even)

greater_10 = [x for x in data if x > 10]
print(greater_10)

less_10 = [x for x in data if x < 10]
print(less_10)

my = ["This","is","Python","tutorial","from","SK. SALAHUDDIN"]
my_list = [i[0] for i in my]
print(my_list)

a = [3, 4, 5]
multiplied = [i*2 for i in a]
print(multiplied)
